.tab-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 100;
  display: flex;
  height: 100rpx;
  border-top: 1rpx solid #ddd;
  background: #fff;
  color: #999;
  line-height: 30rpx;
  font-size: 24rpx;
  text-align: center;
  padding: 5rpx 0 0;

  padding-bottom: env(safe-area-inset-bottom);
}

.flex_hc {
  justify-content: center;
}

.flex_hm {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}

.tab-bar .image {
  width: 50rpx;
  height: 50rpx;
  display: block;
}
.tab-bar .index {
  flex: 1;
  height: 100%;
}

.tab-bar .cur {
  color: #3e75d7;
}
.getUserInfoView {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 9999;
  background: transparent;
  border: none;
}
.getUserInfoView::after{

  border: none;

}